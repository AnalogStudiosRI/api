{
  "name": "api.analogstudios.net",
  "version": "0.3.1",
  "repository": "git@github.com:AnalogStudiosRI/api.git",
  "author": "Owen Buckley <analogstudiosri@gmail.com>",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "arc": "arc",
    "prisma": "prisma",
    "lint": "eslint && tsc --project tsconfig.json",
    "dev": "node --env-file=.env --import @greenwood/cli/register --experimental-strip-types ./node_modules/@greenwood/cli/src/bin.js develop",
    "build": "node --import @greenwood/cli/register --experimental-strip-types ./node_modules/@greenwood/cli/src/bin.js build",
    "serve": "npm run build && node --env-file=.env --experimental-strip-types ./node_modules/@greenwood/cli/src/bin.js serve",
    "deploy:sandbox": "npm run build && arc sandbox",
    "deploy:staging": "npm run arc deploy -- --staging",
    "deploy:prod": "npm run arc deploy -- --production",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.726.0",
    "@contentful/rich-text-html-renderer": "^17.0.0",
    "@libsql/client": "^0.14.0",
    "@prisma/client": "^6.8.2",
    "contentful": "^11.3.0"
  },
  "devDependencies": {
    "@architect/architect": "^11.1.0",
    "@eslint/js": "^9.11.1",
    "@greenwood/cli": "^0.33.0-alpha.4",
    "@greenwood/plugin-adapter-aws": "^0.33.0-alpha.4",
    "eslint": "^9.11.1",
    "prisma": "^6.8.2",
    "typescript": "^5.8.2"
  }
}
