{
  "name": "api.analogstudios.net",
  "version": "0.3.1",
  "repository": "git@github.com:AnalogStudiosRI/api.git",
  "author": "Owen Buckley <analogstudiosri@gmail.com>",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "arc": "arc",
    "clean": "rimraf ./public .greenwood",
    "prisma": "prisma",
    "lint": "eslint",
    "dev": "node --env-file=.env --import @greenwood/cli/register ./node_modules/@greenwood/cli/src/index.js develop",
    "build": "node --import @greenwood/cli/register ./node_modules/@greenwood/cli/src/index.js build",
    "serve": "npm run clean && npm run build && node --env-file=.env ./node_modules/@greenwood/cli/src/index.js serve",
    "deploy:sandbox": "npm run build && arc sandbox",
    "deploy:staging": "npm run arc deploy -- --staging",
    "deploy:prod": "npm run arc deploy -- --production",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.726.0",
    "@contentful/rich-text-html-renderer": "^17.0.0",
    "@libsql/client": "^0.14.0",
    "contentful": "^11.3.0"
  },
  "devDependencies": {
    "@architect/architect": "^10.11.2",
    "@eslint/js": "^9.11.1",
    "@greenwood/cli": "^0.31.0",
    "@greenwood/plugin-typescript": "^0.31.0",
    "eslint": "^9.11.1",
    "prisma": "^5.7.0",
    "rimraf": "^5.0.5"
  }
}
